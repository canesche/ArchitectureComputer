#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x559273e9f060 .scope module, "and_testbench" "and_testbench" 2 1;
 .timescale 0 0;
v0x559273ebf600_0 .var "a", 0 0;
v0x559273ebf6d0_0 .var "b", 0 0;
v0x559273ebf7a0_0 .net "c", 0 0, L_0x559273ebf8a0;  1 drivers
S_0x559273e9f1e0 .scope module, "port" "AND" 2 6, 3 5 0, S_0x559273e9f060;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "in1"
    .port_info 1 /INPUT 1 "in2"
    .port_info 2 /OUTPUT 1 "out"
L_0x559273ebf8a0 .functor AND 1, v0x559273ebf600_0, v0x559273ebf6d0_0, C4<1>, C4<1>;
v0x559273e9f400_0 .net "in1", 0 0, v0x559273ebf600_0;  1 drivers
v0x559273ebf420_0 .net "in2", 0 0, v0x559273ebf6d0_0;  1 drivers
v0x559273ebf4e0_0 .net "out", 0 0, L_0x559273ebf8a0;  alias, 1 drivers
    .scope S_0x559273e9f060;
T_0 ;
    %vpi_call 2 10 "$display", "---Port logic AND---" {0 0 0};
    %delay 10, 0;
    %vpi_call 2 11 "$display", "a b | c" {0 0 0};
    %vpi_call 2 12 "$display", "-------" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559273ebf600_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559273ebf6d0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 13 "$display", "%b %b | %b", v0x559273ebf600_0, v0x559273ebf6d0_0, v0x559273ebf7a0_0 {0 0 0};
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559273ebf600_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559273ebf6d0_0, 0, 1;
    %vpi_call 2 14 "$display", "%b %b | %b", v0x559273ebf600_0, v0x559273ebf6d0_0, v0x559273ebf7a0_0 {0 0 0};
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559273ebf600_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559273ebf6d0_0, 0, 1;
    %vpi_call 2 15 "$display", "%b %b | %b", v0x559273ebf600_0, v0x559273ebf6d0_0, v0x559273ebf7a0_0 {0 0 0};
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559273ebf600_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559273ebf6d0_0, 0, 1;
    %vpi_call 2 16 "$display", "%b %b | %b", v0x559273ebf600_0, v0x559273ebf6d0_0, v0x559273ebf7a0_0 {0 0 0};
    %delay 10, 0;
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "and_testbench.v";
    "and.v";
